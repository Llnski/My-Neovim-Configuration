{
  "svelte.enable-ts-plugin": true,
"coc.preferences.triggerSignatureHelp":true,
"coc.preferences.echodocSupport": true,
  "languageserver": {
    "leanls": {
      "doHover": "LeanAbbreviationsReverseLookup",
      "command": "lean",
      "args": ["--server"],
      "filetypes": ["lean"]
    },
  "prolog-lsp": {
    "command": "swipl",
    "args": ["-g", "use_module(library(lsp_server)).",
             "-g", "lsp_server:main",
             "-t", "halt",
             "--", "stdio"
            ],
    "filetypes": ["prolog"]
  }
}
}
